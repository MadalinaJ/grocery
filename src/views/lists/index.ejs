<!DOCTYPE html>
<html>
  <head>
    <% include ../static/partials/head.ejs %>
  </head>
  <body>
    <main class="container">
      <% include ../static/partials/navbar.ejs %>

<div id="content">
      <div class="container">
        <div class="jumbotron">
      <h1 style="background-color: #2c3e50; color:#20c997;">Saved Lists</h1>

      <ul class="list-group">
          <% lists.forEach((list) => { %>
            <li class="list-group-item">
              <a style="color:#dc3545; font-size: 20px; font-weight: bold;" href="/lists/<%= list.id %>"> <%= list.title %> </a>
            </li>
          <% }) %>
          <a href="/lists/new" class="btn btn-success" style="background-color: #2c3e50; color:#dc3545; font-size:23px;font-weight: bold">. Create New List .</a>

        
        </ul>
    </div>
  </div>
  </div>

    </main>
  </body>
</html>
